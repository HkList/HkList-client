{
  "name": "hklist-client",
  "version": "0.0.8",
  "description": "HkList 客户端程序",
  "main": "./out/main/index.js",
  "author": "huan_kong",
  "homepage": "https://github.com/huankong233/HkList-client",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "chcp.com 65001 && electron-vite dev -w",
    "build": "npm run typecheck && tsx ./scripts/modify.ts && electron-vite build && tsx ./scripts/recovery.ts",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "devDependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/typed-ipc": "^1.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@huan_kong/electron-clipboard-observer": "^0.0.2",
    "@huan_kong/maria2": "^0.3.4",
    "@types/node": "^22.13.1",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.18",
    "@types/ws": "^8.5.14",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "dmg-builder": "^26.0.6",
    "electron": "^34.1.1",
    "electron-builder": "^26.0.6",
    "electron-builder-squirrel-windows": "^26.0.6",
    "electron-context-menu": "^4.0.5",
    "electron-updater": "^6.3.9",
    "electron-vite": "^3.0.0-beta.0",
    "electron-window-state": "^5.0.3",
    "fs-extra": "^11.3.0",
    "is-port-reachable": "^4.0.0",
    "node-machine-id": "^1.1.12",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.0",
    "prettier": "^3.5.0",
    "qs": "^6.14.0",
    "sass-embedded": "^1.83.4",
    "tdesign-icons-vue-next": "^0.3.4",
    "tdesign-vue-next": "^1.10.7",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-vue-components": "^28.0.0",
    "vite": "^6.1.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.0",
    "ws": "^8.18.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "bufferutil",
      "electron",
      "electron-winstaller",
      "esbuild",
      "utf-8-validate"
    ]
  }
}
